{
    "name": "puleeno/wp-crawflow",
    "description": "Use Rake migration framework to migrate other website to WordPress",
    "type": "project",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Puleeno Nguyen",
            "email": "puleeno@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ramphor/rake": "dev-master",
        "puleeno/rake-wordpress-adapter": "dev-master",
        "jbroadway/urlify": "^1.2",
        "symfony/polyfill-php80": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CrawlFlow\\": "app/"
        }
    },
    "config": {
        "allow-plugins": {
            "alleyinteractive/composer-wordpress-autoloader": true
        }
    }
}
